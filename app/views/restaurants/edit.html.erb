<%= form_for @restaurant do |f| %>
<%= f.label :name %>
<%= f.text_field :name %> <br>
<%= f.label :neighborhood %>
<%= f.text_field :neighborhood %> <br>
<%= f.label :street_address %>
<%= f.text_field :street_address %> <br>
<%= f.label :category %>
<%= f.text_field :category %> <br>
<%= f.label :tips %>
<%= f.text_field :tips %> <br>
<%= f.submit %>
<% end %>
